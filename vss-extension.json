{
    "manifestVersion": 1,
    "id": "dev-ops-stream",
    "publisher": "chwebdude",
    "version": "0.0.1",
    "name": "DevOps Stream",
    "description": "A hub which provides a news stream/feed",
    "public": false,
    "categories": ["Azure Boards"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services.Cloud"
        }
    ],
    "contributions": [
        {
            "id": "stream-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-web.home-hub-group"
            ],
            "properties": {
                "name": "Stream",
                "uri": "static/stream-hub.html",
                "iconAsset": "chwebdude.dev-ops-stream/static/images/icon.png",
                "_sharedData": {
                    "assets": [
                        "chwebdude.dev-ops-stream/static/images/icon.png"
                    ]
                }
            }
        }
    ],
    "files": [
        {
            "path": "static",
            "addressable": true
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        },
        {
            "path": "dist",
            "addressable": true
        }
    ]
}